@article{dammaccoProblemsHypoglycemiaArising1998,
  title = {Problems of Hypoglycemia Arising in Children and Adolescents with Insulin-Dependent Diabetes Mellitus. {{The Diabetes Study Group}} of {{The Italian Society}} of {{Pediatric Endocrinology}} \& {{Diabetes}}},
  author = {Dammacco, F. and Torelli, C. and Frezza, E. and Piccinno, E. and Tansella, F.},
