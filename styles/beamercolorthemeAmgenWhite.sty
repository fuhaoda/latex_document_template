\ProvidesPackage{beamercolorthemeAmgenWhite}
% Adapted from the default crane style

\mode<presentation>

\definecolor{EmphColor}{RGB}{153,1,0}
\definecolor{HighlightBackground}{RGB}{255,255,0}
\definecolor{KeywordColor}{RGB}{0,0,95}

% Color definitions
\definecolor{MyFav}{RGB}{0,99,195}

\definecolor{MyMediumGray}{RGB}{165,157,149}
\definecolor{MyRed}{RGB}{213,43,30}
\definecolor{MyRed75}{RGB}{243,106,80}
\definecolor{MyRed50}{RGB}{255,158,135}
\definecolor{MyRed25}{RGB}{255,207,193}

\definecolor{MyGreen}{RGB}{0,175,63}
\definecolor{MyGreen75}{RGB}{9,197,112}
\definecolor{MyGreen50}{RGB}{127,218,158}
\definecolor{MyGreen25}{RGB}{194,237,206}

\definecolor{MyBlue}{RGB}{0,161,222}
\definecolor{MyBlue75}{RGB}{93,184,231}
\definecolor{MyBlue50}{RGB}{156,207,239}
\definecolor{MyBlue25}{RGB}{208,231,247}

\definecolor{MyPurple}{RGB}{177,5,157}
\definecolor{MyPurple75}{RGB}{213,99,185}
\definecolor{MyPurple50}{RGB}{231,153,208}
\definecolor{MyPurple25}{RGB}{245,204,231}


\setbeamercolor*{section text}{fg=white!80!gray}

\setbeamercolor{item projected}{bg=MyFav}
\setbeamercolor{itemize item}{fg=MyFav}
\setbeamercolor{itemize subitem}{fg=MyMediumGray}
\setbeamercolor{description item}{fg=MyFav}
\setbeamercolor{enumerate item}{fg=MyFav}
\setbeamercolor{enumerate subitem}{fg=MyMediumGray}


\setbeamercolor{palette secondary}{fg=white,bg=MyFav}
% This green is just so that I can figure out when, if ever, this
% is used.  If it is a useful palette, it should be changed to something
% else.  Let me know if you figure out what uses these.
\setbeamercolor{palette tertiary}{fg=green,bg=MyMediumGray}
\setbeamercolor{palette quaternary}{fg=white,bg=MyFav}

\setbeamercolor{titlelike}{parent=palette secondary}

\setbeamercolor{block title}{parent=palette secondary}
\setbeamercolor{block title alerted}{use=alerted text,fg=green,bg=alerted text.fg}
\setbeamercolor{block title example}{use=example text,fg=green,bg=example text.fg}

% This ends up applying to most navigation-related things
\setbeamercolor{sidebar}{bg=MyMediumGray}

\setbeamercolor{palette sidebar primary}{fg=white}
\setbeamercolor{palette sidebar secondary}{fg=MyMediumGray}

\setbeamercolor*{separation line}{bg=MyMediumGray}
\setbeamercolor*{fine separation line}{bg=MyMediumGray}


\mode
<all>
